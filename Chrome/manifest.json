{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "0.2.0",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "__MSG_defaultTitle__"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "commands": {
    "hibernateCurrentTab": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "__MSG_commandsDescription__"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "40.0"
}
